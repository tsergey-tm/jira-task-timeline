{
  "manifest_version": 3,
  "name": "Jira Issue timeline viewer",
  "version": "20241030.185836",
  "description": "Show Jira issue timeline",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "Jira Issue timeline viewer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
